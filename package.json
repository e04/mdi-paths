{
  "name": "mdi-paths",
  "version": "0.0.1",
  "description": "This library is a collection of SVG paths to render Material Design Icons in Google Maps API v3, sourced from the MaterialDesign project.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "build": "npx ts-node ./src/build.ts && tsc --project . --module es2015 --outDir ./dist  "
  },
  "keywords": [
    "material design icons",
    "mdi",
    "googlemap",
    "svg",
    "icon"
  ],
  "author": "Takuto AOI",
  "license": "Apache License 2.0",
  "devDependencies": {
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "material-icons": "^0.3.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "xml-js": "^1.6.11"
  }
}
